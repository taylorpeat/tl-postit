<%= render 'shared/content_title', title: "Login" %>

<div class="well">
  <%= form_tag '/login' do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Login", class: 'btn btn-primary' %>
  <% end %>
</div>